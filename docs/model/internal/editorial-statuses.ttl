@prefix acqf-qcp-internal: <https://data.acqf-qcp.africa/model/internal/> .
@prefix :               <https://data.acqf-qcp.africa/model/internal/editorial-statuses/resource/> .
@prefix dct:            <http://purl.org/dc/terms/> .
@prefix rdf:            <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos:           <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:            <http://www.w3.org/2001/XMLSchema#> .

acqf-qcp-internal:editorial-statuses
    a                  skos:ConceptScheme ;
    dct:title          "ACQF QCP Editorial Statuses"@en ;
    dct:description    "This is a SKOS representation of the ACQF QCP Editorial Statuses."@en ;
    dct:created        "2024-10-01"^^xsd:date ;
    dct:publisher      "cognizone bv" ;
    dct:creator        "Petr Křemen" ;
    dct:license        <http://creativecommons.org/publicdomain/zero/1.0/> ;
    skos:hasTopConcept :draft, :under-review-for-obsoletion, :ready-for-obsoletion, :under-review-for-release, :ready-for-release .

:draft
    a              skos:Concept ;
    skos:inScheme  acqf-qcp-internal:editorial-statuses ;
    skos:prefLabel "Draft"@en, "Préliminaire"@fr, "Preliminar"@pt
.

:under-review-for-obsoletion
    a              skos:Concept ;
    skos:inScheme  acqf-qcp-internal:editorial-statuses ;
    skos:prefLabel "Under review for obsoletion"@en, "En révision pour obsolescence"@fr, "Em revisão para obsolescência"@pt
.

:ready-for-obsoletion
    a              skos:Concept ;
    skos:inScheme  acqf-qcp-internal:editorial-statuses ;
    skos:prefLabel "Ready for obsoletion"@en, "Prêt pour obsolescence"@fr, "Pronto para obsolescência"@pt
.

:under-review-for-release
    a              skos:Concept ;
    skos:inScheme  acqf-qcp-internal:editorial-statuses ;
    skos:prefLabel "Under review for release"@en, "En révision pour publication"@fr, "Em revisão para publicação"@pt
.

:ready-for-release
    a              skos:Concept ;
    skos:inScheme  acqf-qcp-internal:editorial-statuses ;
    skos:prefLabel "Ready for release"@en, "Prêt pour publication"@fr, "Pronto para publicação"@pt
.